    <script>
        $('ul.nav li.dropdown').hover(function () {
            $(this).find('.dropdown-menu').stop(true, true).fadeIn();
        }, function () {
            $(this).find('.dropdown-menu').stop(true, true).fadeOut();
        });
    </script>
    <script>
        document.querySelectorAll('.dropdown > a.dropdown-toggle').forEach(function (el) {
            el.addEventListener('click', function (e) {
                if (!this.nextElementSibling.classList.contains('show')) {
                    window.location.href = this.getAttribute('href');
                }
            });
        });
    </script>
<script>
    document.addEventListener("DOMContentLoaded", function () {
        const menuItems = document.querySelectorAll("li[data-course]");

        menuItems.forEach(item => {
            const courseId = item.getAttribute("data-course");
            const link = item.querySelector("a");

            if (courseId && link) {
                link.addEventListener("click", function (e) {
                    e.preventDefault(); // Stop default navigation

                    // Send AJAX request to set session
                    fetch('SetCourseSession.aspx', {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json'
                        },
                        body: JSON.stringify({ courseId: courseId })
                    })
                        .then(response => {
                            if (response.ok) {
                                // Redirect to the page after session is set
                                window.location.href = link.getAttribute("href");
                            } else {
                                console.error("Failed to set session.");
                            }
                        })
                        .catch(error => console.error("Error:", error));
                });
            }
        });
    });
</script>
    <script>
        $(document).ready(function () {
            // Remove old active
            $(".navbar-nav li").removeClass("active");

            // Current page URL
            var currentPage = window.location.pathname.toLowerCase();

            // Check all <a> links
            $(".navbar-nav li a").each(function () {
                var linkPage = $(this).attr("href").toLowerCase();

                if (currentPage.indexOf(linkPage) !== -1) {
                    $(this).parent().addClass("active");
                }
            });
            $(".navbar-nav li.active").closest(".dropdown").addClass("active");
        });
    </script>
</body>
</html>
