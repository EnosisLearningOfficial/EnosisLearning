        /* Sidebar styles */
        #sidebar {
            height: 100vh;
            width: 145px;
            position: fixed;
            top: 15px;
            left: -140px;
            background-color: transparent;
            transition: 0.3s;   
            z-index: 1000;
            padding-top: 60px;
        }

        #sidebar.active {
            left: 0;
        }

        #sidebar a {
            display: block;
            padding: 15px 20px;
            color: #333;
            text-decoration: none;
            font-weight: 500;
        }

        #sidebar a:hover {
            background-color: transparent;
        }
        #sidebar .text
        {
            font-family: 'Montserrat', sans-serif;
            font-style:normal;
        }
        /* Content shift when sidebar opens */
        #content {
            transition: margin-left 0.3s;
            margin-left: 0;
        }

        #content.shift {
            margin-left: 250px;
        }
        .navbar
        {
            height:50px;
        }
        /* Navbar icon spacing */
        .navbar-icons .nav-link {
            font-size: 1.2rem;
            margin-left: 15px;
        }

        /* Hamburger cursor */
        .hamburger-btn {
            font-size: 1.5rem;
            cursor: pointer;
        }
